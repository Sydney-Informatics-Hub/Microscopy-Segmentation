{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "properties": {
    "version": {
      "type": "string"
    },
    "flags": {
      "type": "object"
    },
    "shapes": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "label": {
            "type": "string"
          },
          "text": {
            "type": "string"
          },
          "points": {
            "type": "array",
            "items": {
              "type": "array",
              "items": {
                "type": "number"
              }
            }
          },
          "group_id": {
            "type": ["object", "string", "null"],
            "description": "This can be any type"
          },
          "shape_type": {
            "type": "string"
          },
          "flags": {
            "type": "object"
          }
        },
        "required": ["label", "points", "shape_type"]
      }
    },
    "imagePath": {
      "type": "string"
    },
    "imageData": {
      "type": ["object","null","string"]
    },
    "imageHeight": {
      "type": "number"
    },
    "imageWidth": {
      "type": "number"
    }
  },
  "required": ["version", "shapes", "imagePath", "imageHeight", "imageWidth"]
}
